{
  "name": "dummy",
  "version": "1.0.0",
  "description": "oinwvion",
  "scripts": {
    "start": "webpack-dev-server --port 44301 --https --hot --env=Development --progress",
    "apply": "npm run apply:dev",
    "apply:dev": "SET NODE_ENV=Development&& npm run action:package:dev && npm run action:apply",
    "apply:prod": "SET NODE_ENV=Production&& npm run action:package:prod && npm run action:apply",
    "build": "npm run build:dev",
    "build:dev": "SET NODE_ENV=Development&& npm run action:package:dev",
    "build:prod": "SET NODE_ENV=Production&& npm run action:package:prod",
    "transpile": "webpack --env=Development --progress",
    "action:package:dev": "npm run action:package:app:dev",
    "action:package:prod": "npm run action:package:app:prod",
    "action:package:app:dev": "npm install && webpack --env=Development --progress",
    "action:package:app:prod": "npm install && webpack --env=Production --progress"
  },
  "author": "baywet",
  "license": "UNLICENSED",
